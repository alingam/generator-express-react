{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "dependencies": {<% if (expressVersion === '4.x') { %>
    "express": "~4.0.0-rc2",
    "morgan": "~1.0.0",
    "body-parser": "~1.0.0",
    "compression": "1.0.1",
    "static-favicon": "~1.0.1",
    "method-override": "~1.0.0",
    "errorhandler": "~1.0.0",<% } else { %>
    "express": "~3.4.8",<% } if (viewEngine === 'jade') { %>
    "jade": "~1.1.5"<% } else if (viewEngine === 'hbs'){ %>
    "express-hbs": "~0.7.9"<% } else { %>
    "ejs": "~1.0.0"
  },
  "devDependencies": {<%  if (buildTool === 'grunt') %>
    "grunt": "~0.4.4",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-concurrent": "~0.5.0",
    "grunt-node-inspector": "~0.1.5",
    "grunt-contrib-cssmin": "~0.9.0",
    "matchdep": "~0.3.0",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-contrib-concat": "~0.4.0",
    "grunt-contrib-<%= cssPreprocessor %>":"~0.11.0"
  }
}
